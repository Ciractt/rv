}
table.meta-tier-effect tr:nth-child(2) td:first-child strong {
    background-color: #7bdcb5; /* A tier */
}
table.meta-tier-effect tr:nth-child(3) td:first-child strong {
    background-color: #fcb900; /* B tier */
}
table.meta-tier-effect tr:nth-child(4) td:first-child strong {
    background-color: #ff6900; /* C tier */
}

/* Hover effect for badges */
table.meta-tier-effect td:first-child strong:hover {
    transform: scale(1.1);
    box-shadow: 0 8px 16px rgba(0,0,0,0.2);
}

/* Custom vertical divider in the last cell */
table.meta-tier-effect tr td:last-child {
    border-left: none;
    padding-left: 15px;
    vertical-align: middle;
    position: relative;
}

table.meta-tier-effect tr td:last-child::before {
    content: "";
    position: absolute;
    left: 0;
    top: 0;
    bottom: 0;
    width: 3px;
    background: linear-gradient(to bottom, #ccc, #0077ff, #ccc);
    border-radius: 3px;
}

/* Horizontal separator for each row except the last */
table.meta-tier-effect tbody tr:not(:last-child) {
    position: relative;
}

table.meta-tier-effect tbody tr:not(:last-child)::after {
    content: "";
    position: absolute;
    left: 0;
    bottom: -5px;
    width: 100%;
    height: 2px;
    background: linear-gradient(to right, #ccc, #0077ff, #ccc);
    border-radius: 2px;
}

table.meta-tier-effect {
    display: inline-block; /* Required for background-clip to work properly */
    position: relative !important;
    border-collapse: separate !important;
    border-spacing: 0 !important;
    border-radius: 25px !important;
    /* Force a background on the table element so the gradient border is visible */
	background-image: 
         linear-gradient(45deg, #333, #333) !important;
}




 /*for the main page button hover */
.wp-block-button__link.has-luminous-vivid-orange-background-color.has-background.wp-element-button:hover {
  animation: shakeUp 0.6s ease;
  background-color: #e65c00 !important;  */
}

@keyframes shakeUp {
  0%   { transform: translateY(0) rotate(0deg); }
  20%  { transform: translateY(-4px) rotate(-2deg); }
  40%  { transform: translateY(-2px) rotate(2deg); }
  60%  { transform: translateY(-4px) rotate(-1deg); }
  80%  { transform: translateY(-2px) rotate(1deg); }
  100% { transform: translateY(0) rotate(0deg); }
}


